{
  "env": {
    "browser": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "eol-last": 1,
    "no-console": 0,
    import/no-extraneous-dependencies: [error, { devDependencies: true }],
  },
  "settings": {
    "import/resolver": {
      "babel-module": {},
    },
  },
}
