{
  "name": "rxjs-ajax-wrapper",
  "version": "1.0.1",
  "description": "Simple wrapper using rxjs ajax.",
  "main": "./dist/",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "prepare": "npm run build",
    "lint": "npm run lint:src",
